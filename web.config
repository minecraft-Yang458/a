<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpErrors>
            <remove statusCode="403" subStatusCode="-1" />
            <remove statusCode="404" subStatusCode="-1" />
            <error statusCode="404" prefixLanguageFilePath="\error" path="404.html" responseMode="File" />
            <error statusCode="403" prefixLanguageFilePath="\error" path="403.html" responseMode="File" />
        </httpErrors>
    </system.webServer>
</configuration>
